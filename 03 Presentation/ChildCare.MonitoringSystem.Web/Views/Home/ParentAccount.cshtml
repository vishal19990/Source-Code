                                    </div>
                                    @*<div class="content">
                                            <a class="js-acc-btn" href="#">purshotham</a>
                                        </div>*@
                                    <div class="account-dropdown js-dropdown">
                                        <div class="info clearfix">
                                            <div class="image">
                                                <a href="#">
                                                    <img src="~/images/img/user.png" alt="John Doe" />
                                                </a>
                                            </div>
                                            <div class="content">
                                                <h5 class="name">
                                                    <a href="#">purshotham</a>
                                                </h5>
                                                <span class="email">purshotham123@gmail.com/span>
